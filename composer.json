{
    "name": "mak001/silverstripe-projects",
    "description": "A module for projects",
    "type": "silverstripe-vendormodule",
    "require": {
        "silverstripe/recipe-cms": "^1.0",
        "silverstripe/vendor-plugin": "^1.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.0",
        "tractorcow/silverstripe-colorpicker": "^4@dev",
        "silverstripe/lumberjack": "^2.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "process-timeout": 600
    },
    "autoload": {
        "psr-4": {
            "Mak001\\Portfolio\\": "src/",
            "Mak001\\Portfolio\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "branch-alias": {
        "dev-master": "3.0.x-dev"
    }
}
